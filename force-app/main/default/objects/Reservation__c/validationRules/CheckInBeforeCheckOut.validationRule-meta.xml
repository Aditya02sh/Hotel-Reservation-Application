<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>CheckInBeforeCheckOut</fullName>
    <active>true</active>
    <errorConditionFormula>Check_in_Date__c &gt;= Check_out_Date__c</errorConditionFormula>
    <errorMessage>Check-in date must be before the check-out date</errorMessage>
</ValidationRule>
